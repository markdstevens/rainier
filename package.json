    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chalk": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "clear": "^0.1.0",
    "commander": "^5.1.0",
    "convert-tsconfig-paths-to-webpack-aliases": "0.9.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "css-modules-typescript-loader": "^4.0.0",
    "express": "4.17.3",
    "express-hbs": "2.3.3",
