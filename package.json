{
  "name": "rainier",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Mark Stevens <mdstevens94@gmail.com>",
  "main": "./dist/index",
  "bin": {
    "rainier": "dist/rainier-cli/index.js"
  },
  "scripts": {
    "build": "npm run clean && npm run tsc && cp -R src/rainier-server/views dist/rainier-server && cp -R src/rainier-server/https dist/rainier-server && cp -R src/rainier-cli/sub-commands/init/templates dist/rainier-cli/sub-commands/init/templates",
    "clean": "rimraf dist && rimraf coverage",
    "eslint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "eslint:fix": "npm run eslint -- --quiet --fix",
    "nuke": "npm run clean && rimraf node_modules && rm package-lock.json",
    "tsc": "npm run ts:main && npm run ts:cli && npm run ts:sw",
    "test": "BABEL_ENV=test jest",
    "publish:alpha": "npm run build && npm version --no-git-tag-version prerelease --preid=alpha && npm publish",
    "publish:minor": "npm run build && npm version --no-git-tag-version minor && npm publish",
    "ts:sw": "tsc -p tsconfig-sw.json",
    "ts:cli": "ttsc -p tsconfig-cli.json",
    "ts:main": "ttsc -p tsconfig-non-cli.json",
    "check:ts": "npm run ts:sw --noEmit && npm run ts:cli --noEmit && npm run ts:main --noEmit",
    "check:prettier": "prettier --check 'src/**/*.{js,jsx,ts,tsx}' 'test/**/*.{js,jsx,ts,tsx}'",
    "check:eslint": "eslint 'src/**/*.{js,ts,jsx,tsx}' 'test/**/*.{js,jsx,ts,tsx}' --quiet",
    "code-checks": "npm run check:prettier && npm run check:eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run eslint:fix && npm run check:ts"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-top-level-await": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@loadable/babel-plugin": "^5.13.0",
    "@loadable/component": "^5.13.1",
    "@loadable/server": "^5.13.1",
    "@loadable/webpack-plugin": "^5.13.0",
